    BEGIN
        MENUITEM "Config I&nput...",            ID_SETTINGS_CONFIG_INPUT,MFT_STRING,MFS_ENABLED
        MENUITEM "Config &Video...",            ID_SETTINGS_CONFIG_VIDEO,MFT_STRING,MFS_ENABLED
